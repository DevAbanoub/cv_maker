<p id="notice"><%= notice %></p>

<h1><%= t ".title" %></h1>
<h1><%= t @test %></h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.user_name %></td>
      </tr>
      <tr>
        <td><%= link_to t(".show"), user %></td>
        <td><%= link_to t(".edit"), edit_user_path(user) %></td>
        <td><%= link_to t(".delete"), user, method: :delete, data: {confirm: t(".confirm_msg")} %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t(".new_user"), new_user_path %>
